<jaxb:bindings version="1.0" xmlns:jaxb="http://java.sun.com/xml/ns/jaxb" 
	xmlns:xs="http://www.w3.org/2001/XMLSchema" 
	xmlns:xjc="http://java.sun.com/xml/ns/jaxb/xjc" 
	jaxb:extensionBindingPrefixes="xjc">

	<jaxb:bindings 
		schemaLocation="http://schemas.opengis.net/indoorgml/1.0/indoorgmlnavi.xsd" 
		node="/xs:schema">
		<jaxb:schemaBindings>
			<jaxb:package name="net.opengis.indoorgml.navigation.v_1_0"/>
		</jaxb:schemaBindings>
	</jaxb:bindings>

	<jaxb:bindings 
		schemaLocation="http://schemas.opengis.net/indoorgml/1.0/indoorgmlcore.xsd" 
		node="/xs:schema">
		<jaxb:schemaBindings>
			<jaxb:package name="net.opengis.indoorgml.core.v_1_0"/>
		</jaxb:schemaBindings>
	</jaxb:bindings>
	
	
	<jaxb:bindings
		schemaLocation = "./indoorgmlcore1_0_3_extension.xsd"
		node="/xs:schema">
		<jaxb:schemaBindings>
				<jaxb:package name="net.opengis.indoorgml.ext.v_1_0"/>
		</jaxb:schemaBindings>
	</jaxb:bindings>
	
</jaxb:bindings>